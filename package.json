{
  "name": "metaman",
  "version": "0.1.3",
  "private": true,
  "author": {
    "name": "Yojji",
    "email": "info@yojji.io",
    "url": "https://yojji.io"
  },
  "build": {
    "npmRebuild": false,
    "appId": "com.yojji.metaman",
    "productName": "Metaman",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "antd": "^3.25.0",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.13.0",
    "bootstrap-utilities": "^4.1.3",
    "customize-cra": "^0.9.1",
    "electron-better-ipc": "^0.7.0",
    "electron-is-dev": "^1.1.0",
    "electron-serve": "^0.4.1",
    "env-cmd": "^10.0.1",
    "fela": "^11.0.2",
    "fela-preset-web": "^11.0.2",
    "file-saver": "^2.0.2",
    "firebase": "^7.3.0",
    "fontawesome": "^5.6.3",
    "fractal-objects": "^0.10.3",
    "i18next": "^19.0.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-chained-backend": "^2.0.0",
    "i18next-localstorage-backend": "^3.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "react": "^16.11.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.11.0",
    "react-fela": "^11.0.2",
    "react-highlight": "^0.12.0",
    "react-i18next": "^11.1.0",
    "react-redux": "^7.1.3",
    "react-redux-firebase": "^3.0.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-firestore": "^0.10.0",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "redux-starter-kit": "^1.0.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "type-is": "^1.6.18"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder build -m",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:docker": "env-cmd -f .env.docker react-scripts build",
    "test": "react-app-rewired tests",
    "test:fb-rules": "jest spec/firestore",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|css)\"",
    "lint": "eslint src/**/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.16.4",
    "concurrently": "^5.0.2",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "wait-on": "^3.3.0",
    "uuid": "^3.3.3"
  }
}
